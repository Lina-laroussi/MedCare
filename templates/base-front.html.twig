<!-- Modal for creating or editing rendezVous events -->
<div class="modal fade" id="rendezVousModal" tabindex="-1" role="dialog" aria-labelledby="rendezVousModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="rendezVousModalLabel">Create RendezVous Event</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
    <div class="modal-body">
      <form id="rendezVousForm">

          <div class="form-group">
            <label for="rendezVousStartDate">Start Date</label>
            <input type="text" class="form-control" id="date" name="start" readonly required>
          </div>


          <div class="form-group">
            <label for="rendezVousStartTime">Start Time</label>
            <input type="time" class="form-control" id="heureDebut" name="startTime"  required>
          </div>
        <div class="form-group">
            <label for="rendezVousEndTime">End Time</label>
            <input type="time" class="form-control" id="heureFin" name="endTime"  required>
          </div>
        <div class="form-group">
            <label for="rendezVousSymptomes">Symptomes</label>
            <input type="text" class="form-control" id="symptomes" name="symptomes" required>
          </div>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
          <button type="submit" class="btn btn-primary">Save</button>
        </div>
        <input type="hidden" id="planning-id" name="planning-id" value="" />

      </form>
    </div>
    </div>
  </div>
</div>

