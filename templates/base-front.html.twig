                        <a href="#">Médicaments </a>

                    <li class="has-submenu">
                        <a href="#">Doctoshop </a>
                    </li>
                </ul>
            </div>
            <ul class="nav header-navbar-rht">
                <li class="nav-item contact-item">
                    <div class="header-contact-img">
                        <i class="far fa-hospital"></i>
                    </div>
                    <div class="header-contact-detail">
                        <p class="contact-header">Contact</p>
                        <p class="contact-info-header"> +1 315 369 5943</p>
                    </div>
                </li>

                    {% if is_granted('IS_AUTHENTICATED_FULLY') %}
                        <li class="nav-item dropdown has-arrow logged-item">
                            <a href="#" class="dropdown-toggle nav-link" data-bs-toggle="dropdown">
                                <span class="user-img">
                                <img class="rounded-circle" src="{{ asset('uploads/utilisateurs/' ~ user.image |default('uploads/utilisateurs/user.png') ) }}"  width="31" alt="user image">
                                </span>
                            </a>
                            <div class="dropdown-menu dropdown-menu-end">
                                <div class="user-header">
                                    <div class="avatar avatar-sm">
                                        <img src="{{ asset('uploads/utilisateurs/' ~ user.image |default('uploads/utilisateurs/user.png') ) }}" alt="User Image" class="avatar-img rounded-circle">
                                    </div>
                                    <div class="user-text">
                                        <h6>{{ user.nom |capitalize }} {{ user.prenom |capitalize }}</h6>
                                        {% if is_granted('ROLE_PATIENT') %}
                                        <p class="text-muted mb-0">Patient</p>
                                        {% elseif is_granted('ROLE_MEDECIN') %}
                                        <p class="text-muted mb-0">Médecin</p>
                                        {% elseif is_granted('ROLE_PHARMACIEN') %}
                                        <p class="text-muted mb-0">Pharmacien</p>
                                        {% elseif is_granted('ROLE_ASSUREUR') %}
                                        <p class="text-muted mb-0">Asurreur</p>
                                        {% else %}
                                        <p class="text-muted mb-0">Admin</p>
                                        {% endif %}
                                    </div>
                                </div>
