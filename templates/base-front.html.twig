    {% block notification %}
    {% if is_granted('IS_AUTHENTICATED_FULLY') %}
    <script>
        var userId = {{ user.id }};
const eventSource = new EventSource("{{ mercure('/rdvAjouter')|escape('js') }}");
eventSource.onmessage = event => {
    // Will be called every time an update is published by the server
    const rdv = JSON.parse(event.data);
    const toastHtml = `
        <div class="toast" role="alert" aria-live="assertive" aria-atomic="true">
            <div class="toast-header">
                <strong class="mr-auto">Nouveau RendezVous</strong>
                <small class="text-muted">${rdv.date}</small>
                <button type="button" class="ml-2 mb-1 close" data-dismiss="toast" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="toast-body">
                Vous avez reçu une demande de rendez-vous de ${rdv.nomPatient} pour le ${rdv.date} à ${rdv.heure}.
            </div>
        </div>
    `;
    const toastContainer = document.querySelector(".toast-container");
    toastContainer.innerHTML += toastHtml;

    // Show the toast notification
    const toastElList = [].slice.call(document.querySelectorAll('.toast'))
    const toastList = toastElList.map(toastEl => new bootstrap.Toast(toastEl));
    toastList.forEach(toast => toast.show());

    // Add action to close button
    const closeButtons = document.querySelectorAll(".toast .close");
    closeButtons.forEach(button => {
        button.addEventListener("click", () => {
            const toast = button.closest(".toast");
            const bsToast = bootstrap.Toast.getOrCreateInstance(toast);
            bsToast.hide();
        });
    });


}

    </script>
